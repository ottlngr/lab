                        sh "sleep 4"
                    }
                }
								stage('s2.5') {
                    steps {
                        sh "sleep 10"
                    }
                }
            }
        }
    }
		stage('s3') {
            parallel {
                stage('s3.1') {
                    steps {
                        sh "sleep 1"
                    }
                }
                stage('s3.2') {
                    steps {
                        sh "sleep 2"
                    }
                }
