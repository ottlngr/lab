                    steps {
                        sh "sleep 3"
                    }
                }
            }
        }
    }
		stage('s2') {
            parallel {
                stage('s2.1') {
                    steps {
                        sh "sleep 1"
                    }
                }
                stage('s2.2') {
                    steps {
                        sh "sleep 2"
                    }
                }
								stage('s2.3') {
                    steps {
                        sh "sleep 5"
                    }
                }
								stage('s2.4') {
